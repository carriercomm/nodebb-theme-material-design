<li class="chat-message clear" data-uid="{messages.fromuid}" data-self="{messages.self}" data-break="{messages.newSet}">
	<div class="messages">
		<div class="message left">
			<div class="message-text">{messages.content}</div>
			<img src="{messages.fromUser.picture}" class="user-picture" alt="">
		</div>
	</div>
</li>